{
    "openapi": "3.0.0",
    "info": {
        "title": "SIS",
        "description": "TIS API 文档",
        "version": "0.0.1",
        "contact": {}
    },
    "tags": [
        {
            "name": "common",
            "description": ""
        },
        {
            "name": "health",
            "description": ""
        }
    ],
    "servers": [],
    "components": {
        "securitySchemes": {
            "SessionIdAuth": {
                "type": "apiKey",
                "in": "header",
                "name": "SessionId"
            }
        },
        "schemas": {
            "ApiResponseWrapper": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean"
                    },
                    "errorCode": {
                        "type": "string"
                    },
                    "errorMessage": {
                        "type": "string"
                    },
                    "showType": {
                        "type": "string"
                    }
                },
                "required": [
                    "success"
                ]
            },
            "MenuListDTO": {
                "type": "object",
                "properties": {
                    "key": {
                        "description": "唯一标识",
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "number"
                            }
                        ]
                    },
                    "path": {
                        "type": "string",
                        "description": "路径"
                    },
                    "title": {
                        "type": "string",
                        "description": "显示标题"
                    },
                    "icon": {
                        "type": "string",
                        "description": "Icon"
                    },
                    "children": {
                        "description": "子节点",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/MenuListDTO"
                        }
                    }
                },
                "required": [
                    "key",
                    "title"
                ]
            },
            "OssConfigDto": {
                "type": "object",
                "properties": {
                    "signature": {
                        "type": "string",
                        "description": ""
                    },
                    "policy": {
                        "type": "string",
                        "description": ""
                    },
                    "host": {
                        "type": "string",
                        "description": ""
                    },
                    "OSSAccessKeyId": {
                        "type": "string",
                        "description": ""
                    },
                    "key": {
                        "type": "string",
                        "description": ""
                    },
                    "dirPath": {
                        "type": "string",
                        "description": ""
                    }
                },
                "required": [
                    "signature",
                    "policy",
                    "host",
                    "OSSAccessKeyId",
                    "key",
                    "dirPath"
                ]
            },
            "ClassTypes": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "0",
                        "Enum1": "1",
                        "Enum2": "2"
                    }
                ]
            },
            "DailyCheckAssessmentCategoryLevels": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "1",
                        "Enum1": "2"
                    }
                ]
            },
            "DailyCheckAssessmentCategoryPatterns": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "1",
                        "Enum1": "2"
                    }
                ]
            },
            "DailyCheckRankingModes": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "1",
                        "Enum1": "2"
                    }
                ]
            },
            "DailyCheckScoreTypes": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "0",
                        "Enum1": "1"
                    }
                ]
            },
            "GradeCodes": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "0",
                        "Enum1": "1",
                        "Enum2": "2",
                        "Enum3": "3",
                        "Enum4": "4",
                        "Enum5": "5",
                        "Enum6": "6",
                        "Enum7": "7",
                        "Enum8": "8",
                        "Enum9": "9",
                        "Enum10": "10",
                        "Enum11": "11",
                        "Enum12": "12",
                        "Enum13": "13",
                        "Enum14": "30",
                        "Enum15": "31",
                        "Enum16": "32",
                        "Enum17": "33",
                        "Enum18": "51",
                        "Enum19": "52",
                        "Enum20": "53",
                        "Enum21": "54",
                        "Enum22": "55",
                        "Enum23": "-2",
                        "Enum24": "-1"
                    }
                ]
            },
            "ScoreTypes": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "1",
                        "Enum1": "2"
                    }
                ]
            },
            "StudySections": {
                "type": "string",
                "enum": [
                    {
                        "Enum0": "1",
                        "Enum1": "2",
                        "Enum2": "3",
                        "Enum3": "4",
                        "Enum4": "5"
                    }
                ]
            },
            "EnumListDto": {
                "type": "object",
                "properties": {
                    "ClassTypes": {
                        "$ref": "#/components/schemas/ClassTypes"
                    },
                    "DailyCheckAssessmentCategoryLevels": {
                        "$ref": "#/components/schemas/DailyCheckAssessmentCategoryLevels"
                    },
                    "DailyCheckAssessmentCategoryPatterns": {
                        "$ref": "#/components/schemas/DailyCheckAssessmentCategoryPatterns"
                    },
                    "DailyCheckRankingModes": {
                        "$ref": "#/components/schemas/DailyCheckRankingModes"
                    },
                    "DailyCheckScoreTypes": {
                        "$ref": "#/components/schemas/DailyCheckScoreTypes"
                    },
                    "GradeCodes": {
                        "$ref": "#/components/schemas/GradeCodes"
                    },
                    "ScoreTypes": {
                        "$ref": "#/components/schemas/ScoreTypes"
                    },
                    "StudySections": {
                        "$ref": "#/components/schemas/StudySections"
                    }
                },
                "required": [
                    "ClassTypes",
                    "DailyCheckAssessmentCategoryLevels",
                    "DailyCheckAssessmentCategoryPatterns",
                    "DailyCheckRankingModes",
                    "DailyCheckScoreTypes",
                    "GradeCodes",
                    "ScoreTypes",
                    "StudySections"
                ]
            }
        }
    },
    "paths": {
        "/health": {
            "get": {
                "operationId": "AppController_getHealth",
                "summary": "健康检查",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/ApiResponseWrapper"
                                        },
                                        {
                                            "properties": {
                                                "data": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "health"
                ]
            }
        },
        "/common/GetUserMenuData": {
            "post": {
                "operationId": "CommonController_GetUserMenuData",
                "summary": "获取用户Menu数据",
                "parameters": [
                    {
                        "name": "id",
                        "required": true,
                        "in": "query",
                        "description": "当前用户唯一id",
                        "schema": {
                            "type": "number"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "description": "数据",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/MenuListDTO"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Menu 列表",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/ApiResponseWrapper"
                                        },
                                        {
                                            "properties": {
                                                "data": {
                                                    "type": "array",
                                                    "items": {
                                                        "$ref": "#/components/schemas/MenuListDTO"
                                                    }
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "common"
                ],
                "security": [
                    {
                        "SessionIdAuth": []
                    }
                ]
            }
        },
        "/common/SaveUserMenuData": {
            "post": {
                "operationId": "CommonController_SaveUserMenuData",
                "summary": "保存用户Menu数据",
                "parameters": [
                    {
                        "name": "id",
                        "required": true,
                        "in": "query",
                        "description": "当前用户唯一id",
                        "schema": {}
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/MenuListDTO"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Menu 列表",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/ApiResponseWrapper"
                                        },
                                        {
                                            "properties": {
                                                "data": {
                                                    "type": "array",
                                                    "items": {
                                                        "$ref": "#/components/schemas/MenuListDTO"
                                                    }
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "common"
                ],
                "security": [
                    {
                        "SessionIdAuth": []
                    }
                ]
            }
        },
        "/common/GetOssAccessKey": {
            "get": {
                "operationId": "CommonController_GetOssAccessKey",
                "summary": "获取OSS临时验证",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "OSS 配置",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/ApiResponseWrapper"
                                        },
                                        {
                                            "properties": {
                                                "data": {
                                                    "$ref": "#/components/schemas/OssConfigDto"
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "common"
                ],
                "security": [
                    {
                        "SessionIdAuth": []
                    }
                ]
            }
        },
        "/common/GetEnumList": {
            "get": {
                "operationId": "CommonController_GetEnumList",
                "summary": "获取枚举列表",
                "parameters": [],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "$ref": "#/components/schemas/ApiResponseWrapper"
                                        },
                                        {
                                            "properties": {
                                                "data": {
                                                    "$ref": "#/components/schemas/EnumListDto"
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        }
                    }
                },
                "tags": [
                    "common"
                ],
                "security": [
                    {
                        "SessionIdAuth": []
                    }
                ]
            }
        }
    }
}